        "typescript.format.semicolons": {
          "type": "string",
          "default": "ignore",
          "description": "Defines handling of optional semicolons. Requires using TypeScript 3.7 or newer in the workspace.",
          "scope": "resource",
          "enum": [
            "ignore",
            "insert",
            "remove"
