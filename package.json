        "typescript.suggest.classMemberSnippets.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable snippet completions for class members. Requires using TypeScript 4.5+ in the workspace",
          "scope": "resource"
        },
        "typescript.suggest.jsdoc.generateReturns": {
          "type": "boolean",
          "default": true,
